{
  "name": "pipeline-style",
  "version": "1.0.0",
  "description": "pipeline style",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix",
    "prettier": "prettier --write './test/**/*.js' './test/**/*.css' './test/**/*.less' './test/**/*.scss' './test/**/*.vue' './test/**/*.jsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dukegod/pipeline-style.git"
  },
  "keywords": [
    "pipeline"
  ],
  "author": "hui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dukegod/pipeline-style/issues"
  },
  "homepage": "https://github.com/dukegod/pipeline-style#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "1.17.1"
  },
  "husky": {
    "hooks": {
      "pre-add": "npm run prettier",
      "pre-commit": "'lint-staged', 'npm run prettier'",
      "pre-push": "npm run prettier"
    }
  },
  "lint-staged": {
    "test/**/*.js": "eslint"
  },
  "dependencies": {}
